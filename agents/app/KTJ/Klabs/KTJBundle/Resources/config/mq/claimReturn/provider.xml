<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
	<imports>
		<import resource="error.xml"/>
		<import resource="controller.xml"/>
	</imports>
	<parameters>
		<parameter key="ktj.mq.cookie.storage_name.claim_return">mq.storage.cookie.climeReturn</parameter>
	</parameters>
	<services>
		<service id="klabs_ktj.ktj_provider_mq.provider.claim_return" class="Klabs\KTJBundle\KTJ\Provider\MQ\Provider">
			<call method="setAuthController">
				<argument type="service" id='klabs_ktj.ktj_provider_mq_controller_auth.auth_controller.claim_return'/>
			</call>
			<call method="setSessionController">
				<argument type="service" id='klabs_ktj.ktj_provider_mq_controller_auth.session_controller.claim_return'/>
			</call>
			<call method="setExpressCommitController">
				<argument type="service" id='klabs_ktj.ktj_provider_mq_controller_express_commit.express_commit_controller.claim_return'/>
			</call>
			<call method="setReportController">
				<argument type="service" id='klabs_ktj.ktj_provider_mq_controller_report.report_controller.claim_return'/>
			</call>
			<call method="setQueueController">
				<argument type="service" id='klabs_ktj.ktj_provider_mq_controller_queue.queue_controller.claim_return'/>
			</call>
			<call method="setOrderController">
				<argument type="service" id="klabs_ktj.ktj_provider_mq_controller_order.order_controller.claim_return"/>
			</call>
			<call method="setSeatReserveController">
				<argument type="service" id="klabs_ktj.ktj_provider_mq_controller_report.claim_return_controller.claim_return"/>
			</call>
			<call method="setTicketController">
				<argument type="service" id="klabs_ktj.ktj_provider_mq_controller_ticket.ticket_controller.claim_return"/>
			</call>
			<call method="setReturnController">
				<argument type="service" id="klabs_ktj.ktj_provider_mq_controller_return.return_controller.claim_return"/>
			</call>
			<call method="setKtj">
				<argument type="service" id='Klabs\KTJBundle\KTJ\KTJ'/>
			</call>
			<call method="setMachineKey">
				<argument>%claim_return_machine_key%</argument>
			</call>
			<call method="setCacheAdapter">
				<argument type="service" id="app.cache.default"/>
			</call>
			<call method="setHttpClient">
				<argument type="service" id='eight_points_guzzle.client.api_mq'/>
			</call>
			<call method="setQueueId">
				<argument type="string">_cr</argument>
			</call>
		</service>
		<!-- Event listeners -->
		<service class="Klabs\KTJBundle\KTJ\Provider\MQ\EventListener\CookieEventListener" id="klabs_ktj.ktj_provider_mq_event_listener.cookie_event_listener.claim_return">
			<call method="setCacheAdapter">
				<argument type="service" id="app.cache.default"/>
			</call>
			<call method="setCacheAdapterStorageKey">
				<argument>%ktj.mq.cookie.storage_name.claim_return%</argument>
			</call>
			<tag name="kernel.event_listener" event="ktj.mq.report.station.data.before_cr" method="setProviderCookies"/>
			<tag name="kernel.event_listener" event="ktj.mq.queue.requestItem.before_cr" method="setProviderCookies"/>
			<tag name="kernel.event_listener" event="ktj.mq.queue.confirmItem.before_cr" method="setProviderCookies"/>
			<tag name="kernel.event_listener" event="ktj.mq.queue.getRequestedItems.before_cr" method="setProviderCookies"/>
			<tag name="kernel.event_listener" event="ktj.mq.queue.getItem.before_cr" method="setProviderCookies"/>
			<tag name="kernel.event_listener" event="ktj.mq.report.waitlist.display_terminals.before_cr" method="setProviderCookies"/>
			<tag name="kernel.event_listener" event="ktj.mq.report.ticket.terminal info.before_cr" method="setProviderCookies"/>
			<tag name="kernel.event_listener" event="ktj.mq.queue.confirmBooking.before_cr" method="setProviderCookies"/>
			<tag name="kernel.event_listener" event="ktj.mq.reservation.confirm.before_cr" method="setProviderCookies"/>
			<tag name="kernel.event_listener" event="ktj.mq.reservation.fiscalize.before_cr" method="setProviderCookies"/>
			<tag name="kernel.event_listener" event="ktj.mq.reservation.finalize.before_cr" method="setProviderCookies"/>
			<tag name="kernel.event_listener" event="ktj.mq.reservation.cancel.before_cr" method="setProviderCookies"/>
			<tag name="kernel.event_listener" event="ktj.mq.report.ticket.status.before_cr" method="setProviderCookies"/>
			<tag name="kernel.event_listener" event="ktj.mq.claim_return.requestItem.before_cr" method="setProviderCookies"/>
			<tag name="kernel.event_listener" event="ktj.mq.claim_return.confirm.before_cr" method="setProviderCookies"/>
			<tag name="kernel.event_listener" event="ktj.mq.auth.session.info.before_cr" method="setProviderCookies"/>
			<tag name="kernel.event_listener" event="ktj.mq.report.return.reference.before_cr" method="setProviderCookies"/>
			<tag name="kernel.event_listener" event="ktj.mq.report.return.order.before_cr" method="setProviderCookies"/>
			<tag name="kernel.event_listener" event="ktj.mq.ticket.return.before_cr" method="setProviderCookies"/>
			<tag name="kernel.event_listener" event="ktj.mq.auth.session.open.before_cr" method="setProviderCookies"/>
			<tag name="kernel.event_listener" event="ktj.mq.auth.session.close.before_cr" method="setProviderCookies"/>
		</service>
	</services>
</container>
